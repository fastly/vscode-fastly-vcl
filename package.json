{
  "name": "vscode-vcl",
  "displayName": "Varnish Configuration Language (VCL)",
  "description": "Syntax highlight Varnish Configuration Language (VCL) files",
  "publisher": "Leon Brocard",
  "homepage": "https://github.com/fastly/vscode-vcl",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastly/vscode-vcl.git"
  },
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "scripts": {
    "test": "node ./test/runTest.mjs"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vcl",
        "aliases": [
          "Varnish Configuration Language",
          "vcl"
        ],
        "extensions": [
          ".vcl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vcl",
        "scopeName": "source.vcl",
        "path": "./syntaxes/vcl.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.1.5",
    "mocha": "^10.0.0"
  }
}
