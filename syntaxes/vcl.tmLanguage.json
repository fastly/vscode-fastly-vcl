      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.vcl",
          "match": "\\b(else|elseif|elsif|error|if)\\b"
        },
        {
          "name": "meta.function.vcl",
          "begin": "^\\s*(sub)\\s+([a-zA-Z_0-9]+)",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.vcl"
            },
            "2": {
              "name": "entity.name.function.vcl"
            }
          },
          "end": "{"
        },
        {
          "name": "meta.function.vcl",
          "begin": "^\\s*(return)\\s*\\((deliver|fetch|hash|lookup|pass)\\s*\\)",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.vcl"
            },
            "2": {
              "name": "entity.name.function.vcl"
            }
          },
          "end": ";"
        },
        {
          "name": "keyword.other.vcl",
          "match": "\\b(acl|backend|director|penaltybox|ratecounter|table)\\b"
        },
        {
          "name": "keyword.other.vcl",
          "match": "\\b(add|declare local|error|esi|include|log|remove|restart|return|set|synthetic|synthetic\\.base64|unset)\\b"
        },
        {
          "name": "meta.function.vcl",
          "begin": "^\\s*(call)\\s+([a-zA-Z_0-9]+)",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.vcl"
            },
            "2": {
              "name": "entity.name.function.vcl"
            }
          },
          "end": ";"
        },
        {
          "name": "keyword.operator.vcl",
          "match": "(!|=|==|!=|>=|<=|>|<|~|!~|\\|\\||&&|[+*/%&^-|]=|rol=|ror=|\\|\\|=|\\+)"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.vcl",
          "begin": "{([a-z]+)?\"",
          "end": "\"\\1}",
          "patterns": [
            {
              "name": "constant.character.escape.vcl",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.double.vcl",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.vcl",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.double.vcl",
          "match": "\\bLF\\b"
        }
      ]
    },
    "types": {
      "patterns": [
        {
          "name": "entity.name.type.vcl",
          "match": "\\b(ACL|BACKEND|BOOL|FLOAT|ID|INTEGER|IP|RTIME|STRING|TIME)\\b"
        },
        {
          "name": "storage.type.vcl",
          "match": "\\b(chash|client|fallback|hash|random)\\b"
        }
      ]
    },
    "variables": {
      "patterns": [
        {
          "name": "variable.other.vcl",
          "match": "\\b(bereq|beresp|obj|req|resp)\\.http\\.[a-zA-Z0-9_.-]+(:[a-zA-Z0-9_.-]+)?"
        },
        {
          "name": "variable.other.vcl",
          "match": "\\b(var)\\.[a-zA-Z0-9_-]+"
        },
        {
          "name": "variable.other.vcl",
          "match": "\\bF_[a-zA-Z0-9_-]+"
        },
